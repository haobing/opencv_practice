#include<iostream>
#include<opencv2/opencv.hpp>
using namespace cv;
using namespace std;
int main()
{
        //Point(int x,int y)
        Point pt(100,200);
        cout<<"the point "<<pt.x<<" "<<pt.y<<endl;

        //Size(int width,int height)
        Size size1(150,100);
        cout<<"width: "<<size1.width<<"heigth: "<<size1.height<<" area:"<<size1.area()<<endl;

        //Rect(int x,int y,int width,int height)
        Rect rect1(10,20,150,100);
        cout<<"x:"<<rect1.x<<" y:"<<rect1.y<<" width:"<<rect1.width<<"height:"<<rect1.height<<endl;

        //RotatedRect(Point2f center,Size2f size,float angle)

}
